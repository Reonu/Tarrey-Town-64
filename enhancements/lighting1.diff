--- /dev/null
+++ b/src/engine/level_script.c.orig
@@ -0,0 +1,837 @@
+#include <ultra64.h>
+#ifdef NO_SEGMENTED_MEMORY
+#include <string.h>
+#endif
+
+#include "sm64.h"
+#include "audio/external.h"
+#include "buffers/framebuffers.h"
+#include "buffers/zbuffer.h"
+#include "game/area.h"
+#include "game/game_init.h"
